{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "This portfolio is flavoured by Ayush sanj. Built with vanilla JS.",
  "main": "index.js",
  "scripts": {
    "css": "node-sass --include-path scss pages/home/index.scss pages/home/index.css",
    "uglify-css": "uglifycss pages/home/index.css > pages/home/index.min.css",
    "watch": "nodemon -e scss -x 'npm run css && npm run uglify-css'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayushsanjdev/my-portfolio.git"
  },
  "author": "Ayush sanj",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ayushsanjdev/my-portfolio/issues"
  },
  "homepage": "https://github.com/Ayushsanjdev/my-portfolio#readme",
  "devDependencies": {
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1"
  },
  "dependencies": {
    "uglify-js": "^3.17.4",
    "uglifycss": "^0.0.29"
  }
}
